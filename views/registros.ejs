            <DIV>
                <div class="text-c">
                    <H1>LISTA DE REGISTRO DE SOLICITUDES</H1>
                </div>
            </DIV>
    
    <br>

    <div class="container">
        <h1>Tabla de Solicitudes</h1>
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>ID SOLICITUD</th>
                    <th>ALUMNO</th>
                    <th>RUT ALUMNO</th>
                    <th>CORREO ALUMNO</th>
                    <th>NUMERO DE SALA</th>
                    <th>CODIGO DE SOLICITUD</th>
                    <th>FECHA DE SOLICITUD</th>
                    <th>HORA DE INICIO</th>
                    <th>HORA DE SALIDA</th>
                </tr>
            </thead>
            <tbody>
                <% results.forEach((solicitud) => { %>
                <tr>
                    <td> <%= solicitud.solicitud_id %> </td>
                    <td> <%= solicitud.nombre %> </td>
                    <td> <%= solicitud.rut %> </td>
                    <td> <%= solicitud.correo %> </td>
                    <td> <%= solicitud.numero_sala %> </td>
                    <td > <%= solicitud.codigo_solicitud %> </td>
                    <td> <%= solicitud.fechasoli %> </td>
                    <td> <%= solicitud.hora_inicio %> </td>
                    <td> <%= solicitud.hora_final %> </td>
                </tr>
                <% }) %>
                <!-- Agrega más filas según tus necesidades -->
            </tbody>
        </table>
    </div>


    <div class="containter-fluid p-5 pb-2 fixed-bottom" style="background-color: #3a4cd5; color: white; padding: 20px;bottom: 0; width: 100%;" >
        <div class="row">

            <div class="col-lg-3">
                <h3>Nuestro Nombre</h3>
                <p> Yuntas Asociados </p>
            </div>

            <div class="col-lg-3">
                <h3>Dirección de la empresa</h3>
                <p> Picarquin, Pasaje 1 - #1388 </p>
            </div>

            <div class="col-lg-3">
                <h3>Correo electrónico</h3>
                <p>info@empresa.com</p>
            </div>

            <div class="col-lg-3">
                <h3>Teléfono</h3>
                <p>+1 123-456-7890</p>
            </div>
            

        </div>
    </div>



</body>
